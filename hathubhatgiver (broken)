	ripple(RigsButton)
	RigsFrame_2:TweenPosition(UDim2.new(0,0,0,0),Enum.EasingDirection.Out, Enum.EasingStyle.Quad)
	HatsFrame:TweenPosition(UDim2.new(1,0,0,0),Enum.EasingDirection.Out, Enum.EasingStyle.Quad)
end)  

